<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingSetMakerDef[defName="MapGen_AncientComplexRoomLoot_Default"]/root/options</xpath>
		<value>
			<li>
				<weight>0.10</weight>
				<thingSetMaker Class="ThingSetMaker_StackCount">
					<fixedParams>
						<filter>
							<thingDefs>
								<li>EPOE_MAAI_Chip</li>
							</thingDefs>
						</filter>
						<countRange>1~2</countRange>
					</fixedParams>
				</thingSetMaker>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingSetMakerDef[defName="MapGen_AncientComplexRoomLoot_Better"]/root/options</xpath>
		<value>
			<li>
				<weight>0.25</weight>
				<thingSetMaker Class="ThingSetMaker_StackCount">
					<fixedParams>
						<filter>
							<thingDefs>
								<li>EPOE_MAAI_Chip</li>
							</thingDefs>
						</filter>
						<countRange>2~3</countRange>
					</fixedParams>
				</thingSetMaker>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationInsert">
		<xpath>Defs/ThingSetMakerDef[defName="MapGen_AncientComplexRoomLoot_Better" or defName="MapGen_AncientComplex_SecurityCrate"]/root/options/li/thingSetMaker/fixedParams/filter/categories/li[text()="BodyPartsUltra"]</xpath>
		<value>
			<li>EPIA_OrganicOptimizer</li>
		</value>
	</Operation>

</Patch>