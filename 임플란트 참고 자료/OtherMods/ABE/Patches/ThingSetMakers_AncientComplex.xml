<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingSetMakerDef[defName="MapGen_AncientComplexRoomLoot_Default"]/root/options</xpath>
		<value>
			<li>
				<weight>0.10</weight>
				<thingSetMaker Class="ThingSetMaker_StackCount">
					<fixedParams>
						<filter>
							<thingDefs>
								<li>EPOE_MAAI_Chip</li>
							</thingDefs>
						</filter>
						<countRange>1~2</countRange>
					</fixedParams>
				</thingSetMaker>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationConditional">
		<xpath>Defs/ThingSetMakerDef[defName="MapGen_AncientComplexRoomLoot_Better"]</xpath>
		<match Class="PatchOperationAdd">
			<xpath>Defs/ThingSetMakerDef[defName="MapGen_AncientComplexRoomLoot_Better"]/root/options</xpath>
			<value>
				<li>
					<weight>0.25</weight>
					<thingSetMaker Class="ThingSetMaker_StackCount">
						<fixedParams>
							<filter>
								<thingDefs>
									<li>EPOE_MAAI_Chip</li>
								</thingDefs>
							</filter>
							<countRange>2~3</countRange>
						</fixedParams>
					</thingSetMaker>
				</li>
			</value>
		</match>
	</Operation>

	<Operation Class="PatchOperationConditional">
		<xpath>Defs/ThingSetMakerDef[defName="MapGen_AncientComplexRoomLoot_Better" or defName="MapGen_AncientComplex_SecurityCrate"]</xpath>
		<match Class="PatchOperationConditional">
			<xpath>Defs/ThingSetMakerDef[defName="MapGen_AncientComplexRoomLoot_Better" or defName="MapGen_AncientComplex_SecurityCrate"]/root/options/li/thingSetMaker/fixedParams/filter/categories/li[text()="BodyPartsUltra"]</xpath>
			<match Class="PatchOperationInsert">
				<xpath>Defs/ThingSetMakerDef[defName="MapGen_AncientComplexRoomLoot_Better" or defName="MapGen_AncientComplex_SecurityCrate"]/root/options/li/thingSetMaker/fixedParams/filter/categories/li[text()="BodyPartsUltra"]</xpath>
				<value>
					<li>EPIA_OrganicOptimizer</li>
				</value>
			</match>
			<nomatch Class="PatchOperationSequence">
				<operations>
					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingSetMakerDef[defName="MapGen_AncientComplexRoomLoot_Better"]/root/options</xpath>
						<value>
							<li>
								<weight>0.10</weight>
								<thingSetMaker Class="ThingSetMaker_StackCount">
									<fixedParams>
										<filter>
											<thingDefs>
												<li>ExoskeletonSuit</li>
												<li>EPIA_ProtectiveExoskeleton</li>
												<li>TacticalCorneaImplant</li>
												<li>EPIA_AuxiliaryAI_Worker</li>
												<li>EPIA_AuxiliaryAI_Combat</li>
												<li>EPIA_TacticalBionicEye</li>
												<li>EPOE_OrganicOptimizingNanobots</li>
												<li>EPOE_InstinctOptimizingNanobots</li>
												<li>AIChip</li>
											</thingDefs>
										</filter>
										<countRange>1</countRange>
									</fixedParams>
								</thingSetMaker>
							</li>
						</value>
					</li>
					<li Class="PatchOperationAdd">
						<xpath>Defs/ThingSetMakerDef[defName="MapGen_AncientComplex_SecurityCrate"]/root/options</xpath>
						<value>
							<li>
								<weight>0.60</weight>
								<thingSetMaker Class="ThingSetMaker_StackCount">
									<fixedParams>
										<filter>
											<thingDefs>
												<li>ExoskeletonSuit</li>
												<li>EPIA_ProtectiveExoskeleton</li>
												<li>TacticalCorneaImplant</li>
												<li>EPIA_AuxiliaryAI_Worker</li>
												<li>EPIA_AuxiliaryAI_Combat</li>
												<li>EPIA_TacticalBionicEye</li>
												<li>EPOE_OrganicOptimizingNanobots</li>
												<li>EPOE_InstinctOptimizingNanobots</li>
											</thingDefs>
										</filter>
										<countRange>1</countRange>
									</fixedParams>
								</thingSetMaker>
							</li>
						</value>
					</li>
				</operations>
			</nomatch>
		</match>
	</Operation>

</Patch>