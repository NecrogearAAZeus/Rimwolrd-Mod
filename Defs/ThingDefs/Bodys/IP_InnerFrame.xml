    <!-- 1티어 파츠 -->

    <!-- Combat Inner Frame -->
    <!-- 날카로움 방어 35%, 둔탁함 방어 15%, 열기 방어 20%, 총 적재량 +10KG, 조준 지연시간 -10%, 근접 공격 쿨타임 90%, 이동 +5% -->
    <HediffDef ParentName="ImplantHediffBase">
        <defName>IP_CombatInnerFrame</defName>
        <label>Combat Inner Frame</label>
        <labelNoun>Combat Inner Frame</labelNoun>
        <!-- 변방계 병사에게 설치되는 기초적인 전투 내장형 프레임입니다. 뼈와 근육을 소폭 보강하며, 약간의 장갑판과 자세 안정화 모듈이 장착되어 있습니다. 다른 내장형 프레임과 중복하여 설치할 수 없습니다. -->
        <description>A basic combat Inner Frame commonly installed on frontier soldiers. It slightly reinforces bones and muscles, and comes equipped with light armor plating and a posture stabilization module. Cannot be installed simultaneously with other Inner Frame.</description>
        <descriptionHyperlinks>
            <ThingDef>IP_CombatInnerFrame</ThingDef>
        </descriptionHyperlinks>
        <spawnThingOnRemoved>IP_CombatInnerFrame</spawnThingOnRemoved>
        <stages>
            <li>
                <statOffsets>
                    <ArmorRating_Sharp>0.30</ArmorRating_Sharp>
                    <ArmorRating_Blunt>0.15</ArmorRating_Blunt>
                    <ArmorRating_Heat>0.20</ArmorRating_Heat>
                    <VEF_MassCarryCapacity>10</VEF_MassCarryCapacity>
                    <MeleeCooldownFactor>-0.1</MeleeCooldownFactor>
                    <AimingDelayFactor>-0.1</AimingDelayFactor>
                </statOffsets>
                <capMods>
                    <li>
                        <capacity>Moving</capacity>
                        <offset>0.05</offset>
                    </li>
                </capMods>
            </li>
        </stages>
        <comps>
            <li Class="Implant_Plus.HediffCompProperties_EMPReaction" />
        </comps>
        <spawnThingOnRemoved>IP_CombatInnerFrame</spawnThingOnRemoved>
    </HediffDef>

